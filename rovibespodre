-- MADE BY FLICKZ

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TeleportService = game:GetService("TeleportService")

local plr = Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()

-- REINJETAR APÃ“S TROCAR DE SERVER (5s)
if queue_on_teleport then
	queue_on_teleport([[
		wait(5)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/flickzzzzzzzzz/mushyo/main/mushok"))()
	]])
end

-- GUI
local gui = Instance.new("ScreenGui")
gui.ResetOnSpawn = false
gui.Parent = plr:WaitForChild("PlayerGui")

local txt = Instance.new("TextLabel", gui)
txt.Size = UDim2.new(0,260,0,40)
txt.Position = UDim2.new(0,10,0,10)
txt.BackgroundTransparency = 0.3
txt.BackgroundColor3 = Color3.new(0,0,0)
txt.TextColor3 = Color3.new(1,1,1)
txt.TextScaled = true
txt.Font = Enum.Font.GothamBold
txt.Text = "MADE BY FLICKZ"

-- NOCLIP
local noclipConn
noclipConn = RunService.Stepped:Connect(function()
	for _,v in pairs(char:GetDescendants()) do
		if v:IsA("BasePart") then
			v.CanCollide = false
		end
	end
end)

-- CHECK LIGHTS
local function getLights()
	local lights = {}
	if workspace:FindFirstChild("LightsLocal") then
		for _,obj in pairs(workspace.LightsLocal:GetChildren()) do
			if obj:IsA("Model") and obj.Name == "LightTemplate" then
				table.insert(lights, obj)
			end
		end
	end
	return lights
end

-- MAIN LOOP
while true do
	local lights = getLights()

	if #lights == 0 then
		if noclipConn then noclipConn:Disconnect() end
		task.wait(1)
		TeleportService:Teleport(game.PlaceId, plr)
		break
	end

	for _,light in ipairs(lights) do
		if light and light.PrimaryPart then
			char:PivotTo(light:GetPivot() + Vector3.new(0,3,0))
			task.wait(0.5) -- delay entre TPs
		end
	end
end
